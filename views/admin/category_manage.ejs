<body id="category_manage">
    <!-- component -->

    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700&display=swap" rel="stylesheet">

    <!--Content space -->
    <div class="flex-1  bg-main-white">
        <!-- Heading nav -->
        <div class="pt-6 h-12">
            <div class="px-6 py-2 flex justify-between items-center">
                <h1 class="text-2xl font-semibold text-gray-900 ">Category Management</h1>
            </div>
        </div>

        <!-- breadcrumbs -->

        <div class="flex items-center justify-between pt-8 pb-4 px-6">
            <nav class="flex" aria-label="Breadcrumb">
                <ol class="inline-flex items-center space-x-1 md:space-x-2 rtl:space-x-reverse">
                    <li class="inline-flex items-center">
                        <a href="/admin/dashboard"
                            class="inline-flex items-center text-sm font-medium text-button-color hover:text-blue-600 dark:text-gray-400 dark:hover:text-white">
                            Dashboard
                        </a>
                    </li>
                    <li aria-current="page">
                        <div class="flex items-center">
                            <svg class="rtl:rotate-180 w-3 h-3 text-gray-400 mx-1" aria-hidden="true"
                                xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 6 10">
                                <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round"
                                    stroke-width="2" d="m1 9 4-4-4-4" />
                            </svg>
                            <span class="ms-1 text-sm font-medium text-gray-500 md:ms-2 dark:text-gray-400">Category
                                Management</span>
                        </div>
                    </li>
                </ol>
            </nav>
            <!-- add category  -->
            <div class="flex space-x-4">
                <button type="button" data-modal-target="category_add_modal" data-modal-toggle="category_add_modal"
                    class="flex space-x-4 focus:outline-none text-white bg-secondry-button  hover: focus:ring-4 focus:ring-secondry-button font-medium rounded-lg text-sm px-4 py-2.5 mb-2">
                    <span class="pr-2"><img src="/Icons/plus.svg" alt=""></span>Add Category
                </button>


                <!-- Add modal -->

                <div id=category_add_modal aria-hidden="true"
                    class="hidden overflow-y-auto overflow-x-hidden p-6 fixed top-0 right-0 left-0 z-50 justify-center items-center w-full md:inset-0 h-[calc(100%-1rem)] max-h-full">
                    <div class="relative p-4 w-full max-w-md max-h-full">
                        <!-- Modal content -->
                        <div class="relative bg-white rounded-lg shadow dark:bg-gray-700">
                            <!-- Modal header -->
                            <div
                                class="flex items-center justify-between p-4 md:p-5 border-b rounded-t dark:border-gray-600">
                                <h3 class="text-lg font-semibold text-gray-900 dark:text-white">
                                    Add New Category
                                </h3>
                                <button type="button"
                                    class="text-gray-400 bg-transparent hover:bg-gray-200 hover:text-gray-900 rounded-lg text-sm w-8 h-8 ms-auto inline-flex justify-center items-center dark:hover:bg-gray-600 dark:hover:text-white"
                                    data-modal-toggle="category_add_modal">
                                    <svg class="w-3 h-3" aria-hidden="true" xmlns="http://www.w3.org/2000/svg"
                                        fill="none" viewBox="0 0 14 14">
                                        <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round"
                                            stroke-width="2" d="m1 1 6 6m0 0 6 6M7 7l6-6M7 7l-6 6" />
                                    </svg>
                                    <span class="sr-only">Close modal</span>
                                </button>
                            </div>
                            <!-- Modal body -->
                            <form class="p-4 md:p-5" id="editUserForm" action="/admin/add-category" method="post">
                                <input type="hidden" id="category_add_modal">
                                <div class="grid gap-4 mb-4 grid-cols-2">
                                    <div class="col-span-2">
                                        <label for="name"
                                            class="block mb-2 text-sm font-medium text-gray-900 dark:text-white">Category
                                            Name</label>
                                        <input type="text" name="categoryName" id="category_add_modal"
                                            class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-primary-600 focus:border-primary-600 block w-full p-2.5 dark:bg-gray-600 dark:border-gray-500 dark:placeholder-gray-400 dark:text-white dark:focus:ring-primary-500 dark:focus:border-primary-500">
                                    </div>

                                    <div class="col-span-2">
                                        <label for="description"
                                            class="block mb-2 text-sm font-medium text-gray-900 dark:text-white">Category
                                            Description</label>
                                        <textarea id="editDescription" rows="4" name="description"
                                            class="block p-2.5 w-full text-sm text-gray-900 bg-gray-50 rounded-lg border border-gray-300 focus:ring-blue-500 focus:border-blue-500 dark:bg-gray-600 dark:border-gray-500 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500"></textarea>
                                    </div>

                                </div>
                                <button type="submit"
                                    class="text-white inline-flex items-center bg-blue-700 hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800">
                                    <svg class="me-1 -ms-1 w-5 h-5" fill="currentColor" viewBox="0 0 20 20"
                                        xmlns="http://www.w3.org/2000/svg">
                                        <path fill-rule="evenodd"
                                            d="M10 5a1 1 0 011 1v3h3a1 1 0 110 2h-3v3a1 1 0 11-2 0v-3H6a1 1 0 110-2h3V6a1 1 0 011-1z"
                                            clip-rule="evenodd"></path>
                                    </svg>
                                    Save Changes
                                </button>
                            </form>
                        </div>
                    </div>
                </div>

            </div>

        </div>

        <!-- page content -->
        <div
            class=" px-6 py-5 flex items-center justify-between flex-column flex-wrap md:flex-row space-y-4 md:space-y-0 pb-4">
            <div>
                <label for="table-search" class="sr-only">Search</label>
                <div class="relative">
                    <div class="absolute inset-y-0 rtl:inset-r-0 start-0 flex items-center ps-3 pointer-events-none">
                        <svg class="w-4 h-4 text-gray-500 dark:text-gray-400" aria-hidden="true"
                            xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 20 20">
                            <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                                d="m19 19-4-4m0-7A7 7 0 1 1 1 8a7 7 0 0 1 14 0Z" />
                        </svg>
                    </div>
                    <input type="text" id="table-search-category"
                        class="block p-2 ps-10 text-sm text-gray-900 border border-gray-300 rounded-lg w-80 bg-gray-50 focus:ring-blue-500 focus:border-blue-500 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500"
                        placeholder="Search Categories">
                </div>

            </div>

            <!--  sort -->
            <div class="relative inline-block mr-20">
                <button id="dropdownActionButton" data-dropdown-toggle="dropdownAction"
                    class="inline-flex items-center text-gray-500 bg-white border border-gray-300 focus:outline-none hover:bg-gray-100 focus:ring-4 focus:ring-gray-100 font-medium rounded-lg text-sm px-3 py-1.5 dark:bg-gray-800 dark:text-gray-400 dark:border-gray-600 dark:hover:bg-gray-700 dark:hover:border-gray-600 dark:focus:ring-gray-700"
                    type="button"><img src="/Icons/slider.svg" alt="" class="pr-2">
                    Sort
                </button>

                <!-- Dropdown menu -->
                <div id="dropdownRadioBgHover"
                    class="absolute z-10 hidden w-48 bg-white divide-y divide-gray-100 rounded-lg shadow dark:bg-gray-700 dark:divide-gray-600 top-full mt-1">
                    <ul class="p-3 space-y-1 text-sm text-gray-700 dark:text-gray-200"
                        aria-labelledby="dropdownActionButton">
                        <li>
                            <div class="flex items-center p-2 rounded hover:bg-gray-100 dark:hover:bg-gray-600">
                                <input checked id="last-modified" type="radio" value="" name="sort-option"
                                    class="w-4 h-4 text-button-color bg-gray-100 border-gray-300 focus:ring-button-color dark:focus:ring-buttontext-button-color dark:ring-offset-gray-700 dark:focus:ring-offset-gray-700 focus:ring-2 dark:bg-gray-600 dark:border-gray-500">
                                <label for="last-modified"
                                    class="w-full ms-2 text-sm font-medium text-gray-900 rounded dark:text-gray-300">Last
                                    Modified</label>
                            </div>
                        </li>
                        <li>
                            <div class="flex items-center p-2 rounded hover:bg-gray-100 dark:hover:bg-gray-600">
                                <input id="ascending" type="radio" value="" name="sort-option"
                                    class="w-4 h-4 text-button-color bg-gray-100 border-gray-300 focus:ring-button-color dark:focus:ring-buttontext-button-color dark:ring-offset-gray-700 dark:focus:ring-offset-gray-700 focus:ring-2 dark:bg-gray-600 dark:border-gray-500">
                                <label for="ascending"
                                    class="w-full ms-2 text-sm font-medium text-gray-900 rounded dark:text-gray-300">Ascending</label>
                            </div>
                        </li>
                        <li>
                            <div class="flex items-center p-2 rounded hover:bg-gray-100 dark:hover:bg-gray-600">
                                <input id="descending" type="radio" value="" name="sort-option"
                                    class="w-4 h-4 text-button-color bg-gray-100 border-gray-300 focus:ring-button-color dark:focus:ring-buttontext-button-color dark:ring-offset-gray-700 dark:focus:ring-offset-gray-700 focus:ring-2 dark:bg-gray-600 dark:border-gray-500">
                                <label for="descending"
                                    class="w-full ms-2 text-sm font-medium text-gray-900 rounded dark:text-gray-300">Descending</label>
                            </div>
                        </li>
                        <li>
                            <div class="flex items-center p-2 rounded hover:bg-gray-100 dark:hover:bg-gray-600">
                                <input id="active" type="radio" value="" name="sort-option"
                                    class="w-4 h-4 text-button-color bg-gray-100 border-gray-300 focus:ring-button-color dark:focus:ring-buttontext-button-color dark:ring-offset-gray-700 dark:focus:ring-offset-gray-700 focus:ring-2 dark:bg-gray-600 dark:border-gray-500">
                                <label for="active"
                                    class="w-full ms-2 text-sm font-medium text-gray-900 rounded dark:text-gray-300">Active</label>
                            </div>
                        </li>
                        <li>
                            <div class="flex items-center p-2 rounded hover:bg-gray-100 dark:hover:bg-gray-600">
                                <input id="sort-radio" type="radio" value="" name="sort-option"
                                    class="w-4 h-4 text-button-color bg-gray-100 border-gray-300 focus:ring-button-color dark:focus:ring-buttontext-button-color dark:ring-offset-gray-700 dark:focus:ring-offset-gray-700 focus:ring-2 dark:bg-gray-600 dark:border-gray-500">
                                <label for="blocked"
                                    class="w-full ms-2 text-sm font-medium text-gray-900 rounded dark:text-gray-300">Blocked</label>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>



        </div>

        <div class=" bg-white rounded-lg shadow overflow-x-auto">

            <!-- table -->

            <table class="w-full text-sm text-left rtl:text-right  text-gray-500" id="CategoryTable">
                <thead class="text-xs text-gray-700 uppercase  bg-purple-50 dark:bg-gray-700">
                    <tr>
                        <th scope="col" class="p-4">
                            SNo.
                        </th>
                        <th scope="col" class="px-12 py-3 w-10">
                            Category
                        </th>
                        <th scope="col" class="px-4 py-3">
                            Stock
                        </th>
                        <th scope="col" class="px-6 py-3">
                            Sales
                        </th>

                        <th scope="col" class="px-9 py-3">
                            Status
                        </th>
                        <th scope="col" class="px-6 py-3">
                            Created At
                        </th>
                        <th scope="col" class="px-6 py-3">
                            Action
                        </th>
                    </tr>
                </thead>

                <% if (categories && categories.length> 0) { %>
                    <% categories.forEach((category, index)=> { %>

                        <!-- Category View Modal -->

                        <div id="category_view<%= index %>" tabindex="-1" aria-hidden="true"
                            class="hidden overflow-y-auto overflow-x-hidden fixed top-0 right-0 left-0 z-50 justify-center items-center w-full md:inset-0 h-[calc(100%-1rem)] max-h-full">
                            <div class="relative p-4 w-full max-w-md max-h-full">
                                <!-- Modal content -->
                                <div class="relative bg-white rounded-lg shadow dark:bg-gray-700">
                                    <!-- Modal header -->
                                    <div
                                        class="flex items-center justify-between p-4 md:p-5 border-b rounded-t dark:border-gray-600">
                                        <h3 class="text-lg font-semibold text-gray-900 dark:text-white">
                                            category Details
                                        </h3>
                                        <button type="button"
                                            class="text-gray-400 bg-transparent hover:bg-gray-200 hover:text-gray-900 rounded-lg text-sm w-8 h-8 ms-auto inline-flex justify-center items-center dark:hover:bg-gray-600 dark:hover:text-white"
                                            data-modal-toggle="category_view<%= index %>">
                                            <svg class="w-3 h-3" aria-hidden="true" xmlns="http://www.w3.org/2000/svg"
                                                fill="none" viewBox="0 0 14 14">
                                                <path stroke="currentColor" stroke-linecap="round"
                                                    stroke-linejoin="round" stroke-width="2"
                                                    d="m1 1 6 6m0 0 6 6M7 7l6-6M7 7l-6 6" />
                                            </svg>
                                            <span class="sr-only">Close modal</span>
                                        </button>
                                    </div>
                                    <!-- Modal body -->
                                    <div class="p-4 md:p-5">
                                        <div class="grid gap-4 mb-4 grid-cols-2">
                                            <div class="col-span-2">
                                                <p class="text-sm font-medium text-gray-900 dark:text-white">
                                                    Category Name:</p>
                                                <p class="text-sm text-gray-700 dark:text-gray-300">
                                                    <%= category.categoryName %>
                                                </p>
                                            </div>

                                            <div class="col-span-2">
                                                <p class="text-sm font-medium text-gray-900 dark:text-white">
                                                    Category Created:</p>
                                                <p class="text-sm text-gray-700 dark:text-gray-300">
                                                    <%= new Date(category.createdAt).toLocaleString('en-GB', {
                                                        year: 'numeric' , month: '2-digit' , day: '2-digit'
                                                        ,hour: '2-digit' ,minute: '2-digit' ,hour12: false }) %>
                                                </p>
                                            </div>

                                            <div class="col-span-2 sm:col-span-1">
                                                <p class="text-sm font-medium text-gray-900 dark:text-white">
                                                    Deleted:</p>
                                                <p class="text-sm text-gray-700 dark:text-gray-300">
                                                    <% if (category.isDeleted===true) { %>
                                                        Yes
                                                        <% } else { %>
                                                            No
                                                            <% } %>
                                                </p>
                                            </div>
                                            <div class="col-span-2">
                                                <p class="text-sm font-medium text-gray-900 dark:text-white">
                                                    Description:</p>
                                                <p class="text-sm text-gray-700 dark:text-gray-300"
                                                    id="viewDescription">
                                                    <%= category.description%>
                                                </p>
                                            </div>
                                        </div>
                                        <div class="flex justify-end">
                                            <button type="button"
                                                class="text-white bg-blue-700 hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800"
                                                data-modal-toggle="category_view<%= index %>">
                                                Close
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>



                        <tr
                            class="bg-white border-b dark:bg-gray-800 dark:border-gray-700 hover:bg-gray-50 dark:hover:bg-gray-600">
                            <th class="w-1 px-6">
                                <%= index + 1 %>
                            </th>

                            <th scope="row"
                                class="flex items-center px-2 py-4 text-gray-900 whitespace-nowrap dark:text-white category">
                                <div class="h-10 rounded-lg bg-gray-600"></div>
                                <div class="ps-6">
                                    <div class="text-base font-semibold">
                                        <%= category.categoryName %>
                                    </div>
                                    <div
                                        style="font-weight: normal; color: #6b7280; width: 18rem; word-wrap: break-word; word-break: break-word; white-space: normal;">
                                        <%= category.description %>
                                    </div>
                                </div>
                            </th>
                            <td class="px-6 py-4">
                                0
                            </td>
                            <td class="px-6 py-4">
                                â‚¹0
                            </td>

                            <td class="px-6 py-4">
                                <% if (category.isDeleted) { %>
                                    <div
                                        class="h-7 px-3 py-1 bg-red-100 rounded-[100px] flex-col justify-center items-center gap-2 inline-flex">
                                        <div
                                            class="text-center text-red-500 text-sm font-semibold font-inter leading-tight tracking-tight p-1.5">
                                            Deleted</div>
                                    </div>

                                    <% } else { %>
                                        <div
                                            class="h-7 px-3 py-1 bg-green-100 rounded-[100px] flex-col justify-center items-center gap-2 inline-flex">
                                            <div
                                                class="text-center text-green-500 text-sm font-semibold font-inter leading-tight tracking-tight p-1.5">
                                                Active</div>
                                        </div>
                                        <% } %>
                            </td>
                            <td class="px-6 py-4">
                                <%= new Date(category.createdAt).toLocaleString('en-GB', { year: 'numeric' ,
                                    month: '2-digit' , day: '2-digit' ,hour: '2-digit' ,minute: '2-digit' ,hour12: false
                                    }) %>
                            </td>
                            <td class="px-6 pb-10 flex space-x-2">

                                <!-- Edit modal -->

                                <div id=editCategoryModal<%=index %> aria-hidden="true"
                                    class="hidden overflow-y-auto overflow-x-hidden p-6 fixed top-0 right-0 left-0 z-50
                                    justify-center items-center w-full md:inset-0 h-[calc(100%-1rem)] max-h-full">
                                    <div class="relative p-4 w-full max-w-md max-h-full">
                                        <!-- Modal content -->
                                        <div class="relative bg-white rounded-lg shadow dark:bg-gray-700">
                                            <!-- Modal header -->
                                            <div
                                                class="flex items-center justify-between p-4 md:p-5 border-b rounded-t dark:border-gray-600">
                                                <h3 class="text-lg font-semibold text-gray-900 dark:text-white">
                                                    Edit Category
                                                </h3>
                                                <button type="button"
                                                    class="text-gray-400 bg-transparent hover:bg-gray-200 hover:text-gray-900 rounded-lg text-sm w-8 h-8 ms-auto inline-flex justify-center items-center dark:hover:bg-gray-600 dark:hover:text-white"
                                                    data-modal-toggle="editCategoryModal<%= index %>">
                                                    <svg class="w-3 h-3" aria-hidden="true"
                                                        xmlns="http://www.w3.org/2000/svg" fill="none"
                                                        viewBox="0 0 14 14">
                                                        <path stroke="currentColor" stroke-linecap="round"
                                                            stroke-linejoin="round" stroke-width="2"
                                                            d="m1 1 6 6m0 0 6 6M7 7l6-6M7 7l-6 6" />
                                                    </svg>
                                                    <span class="sr-only">Close modal</span>
                                                </button>
                                            </div>
                                            <!-- Modal body -->
                                            <form class="p-4 md:p-5" id="editUserForm" action="/admin/edit-category"
                                                method="post">
                                                <input type="hidden" id="editcategoryId" name="id"
                                                    value="<%= category._id %>">
                                                <div class="grid gap-4 mb-4 grid-cols-2">
                                                    <div class="col-span-2">
                                                        <label for="name"
                                                            class="block mb-2 text-sm font-medium text-gray-900 dark:text-white">Category
                                                            Name</label>
                                                        <input type="text" name="categoryName" id="editCategoryName"
                                                            value="<%= category.categoryName %>"
                                                            class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-primary-600 focus:border-primary-600 block w-full p-2.5 dark:bg-gray-600 dark:border-gray-500 dark:placeholder-gray-400 dark:text-white dark:focus:ring-primary-500 dark:focus:border-primary-500">
                                                    </div>

                                                    <div class="col-span-2">
                                                        <label for="description"
                                                            class="block mb-2 text-sm font-medium text-gray-900 dark:text-white">Category
                                                            Description</label>
                                                        <textarea id="editDescription" rows="4" name="description"
                                                            class="block p-2.5 w-full text-sm text-gray-900 bg-gray-50 rounded-lg border border-gray-300 focus:ring-blue-500 focus:border-blue-500 dark:bg-gray-600 dark:border-gray-500 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500"><%= category.description %></textarea>
                                                    </div>

                                                </div>
                                                <button type="submit"
                                                    class="text-white inline-flex items-center bg-blue-700 hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800">
                                                    <svg class="me-1 -ms-1 w-5 h-5" fill="currentColor"
                                                        viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg">
                                                        <path fill-rule="evenodd"
                                                            d="M10 5a1 1 0 011 1v3h3a1 1 0 110 2h-3v3a1 1 0 11-2 0v-3H6a1 1 0 110-2h3V6a1 1 0 011-1z"
                                                            clip-rule="evenodd"></path>
                                                    </svg>
                                                    Save Changes
                                                </button>
                                            </form>
                                        </div>
                                    </div>
                                </div>

                                <!-- view button -->

                                <button data-modal-target="category_view<%= index %>"
                                    data-modal-toggle="category_view<%= index %>" type="button"> <img
                                        src="/Icons/eye.svg" alt=""></button>

                                <!--edit button -->

                                <button class="edit-btn" data-modal-target="editCategoryModal<%= index %>"
                                    data-modal-toggle="editCategoryModal<%= index %>" type="button ">
                                    <img src="/Icons/pen.svg" alt="" style="height: 20px;">
                                </button>
                                <!-- delete button -->
                                <button class="delete-btn" onclick="deleteCategory('<%= category._id %>')">
                                    <img src="<%= category.isDeleted ? '/Icons/recover.svg' : '/Icons/trash.svg' %>"
                                        alt="Delete">
                                </button>
                            </td>
                        </tr>


                        <% }) %>
                            <% } else { %>
                                <tr>
                                    <td colspan="3">No Categories found.</td>
                                </tr>
                                <% } %>


                                    </tbody>
            </table>
        </div>

    
            <!-- Pagination -->

            <div class="flex items-center justify-between border-t border-gray-200  px-4 py-3 sm:px-6">
                <div class="flex flex-1 justify-between sm:hidden">
                    <a href="#"
                        class="relative inline-flex items-center rounded-md border border-gray-300 bg-white px-4 py-2 text-sm font-medium text-gray-700 hover:bg-gray-50">Previous</a>
                    <a href="#"
                        class="relative ml-3 inline-flex items-center rounded-md border border-gray-300 bg-white px-4 py-2 text-sm font-medium text-gray-700 hover:bg-gray-50">Next</a>
                </div>
                <div class="hidden sm:flex sm:flex-1 sm:items-center sm:justify-between">
                    <div>
                        <p class="text-sm text-gray-700">
                            Showing
                            <span class="font-medium">1</span>
                            to
                            <span class="font-medium">10</span>
                            of
                            <span class="font-medium">97</span>
                            details
                        </p>
                    </div>
                    <div class="mt-2 flex justify-end">
                        <nav class="relative z-0 inline-flex rounded-md shadow-sm -space-x-px">
                            <a href="#"
                                class="px-3 py-2 rounded-l-md border bg-white text-gray-500 hover:bg-gray-50">Previous</a>
                            <a href="#" class="px-3 py-2 border bg-indigo-50 text-button-color">1</a>
